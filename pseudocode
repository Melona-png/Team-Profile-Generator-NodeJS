// Define a function to get employee information from command line
function getEmployeeInfo() {
  // Prompt user for employee information (name, id, email, and role)
  // Store the employee information in an array of objects
  // Return the array of employee objects
}

// Define a function to generate HTML page
function generateHTML(employees) {
  // Initialize a string to store the HTML content
  let htmlContent = '';

  // Loop through each employee object in the array
  for (let employee of employees) {
    // Add employee information to the htmlContent string in the format of a summary
    htmlContent += `<div>
                      <h2>${employee.name}</h2>
                      <p>ID: ${employee.id}</p>
                      <p>Email: ${employee.email}</p>
                      <p>Role: ${employee.role}</p>
                    </div>`;
  }

  // Wrap the htmlContent with HTML structure
  htmlContent = `<html>
                  <head>
                    <title>Software Engineering Team</title>
                  </head>
                  <body>
                    ${htmlContent}
                  </body>
                 </html>`;

  // Write the htmlContent to a file named "team.html"
  // Return the file name
}

// Call the getEmployeeInfo function to get the employee information
let employees = getEmployeeInfo();

// Call the generateHTML function and pass the employee information
let fileName = generateHTML(employees);

// Log the file name to the console
console.log(`HTML page generated: ${fileName}`);